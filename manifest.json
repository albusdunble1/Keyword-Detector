{
  "name": "Getting Started Example",
  "version": "1.0",
  "description": "Build an Extension!",
  "permissions": ["storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "manifest_version": 2,
  "content_scripts" : [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-3.4.1.min.js", "content.js"],
      "css": ["styles.css"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Question Detector"
  }
}